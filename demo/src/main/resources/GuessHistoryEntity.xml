<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.homework.entity.GuessHistoryEntity">
        <table name="guess_history" schema="public" catalog="game"/>
        <attributes>
            <id name="uuid">
                <column name="uuid"/>
            </id>
            <basic name="guessNumber">
                <column name="guess_number"/>
            </basic>
            <basic name="answer">
                <column name="answer"/>
            </basic>
            <basic name="playerId">
                <column name="player_id"/>
            </basic>
            <basic name="gameId">
                <column name="game_id"/>
            </basic>
            <basic name="result">
                <column name="result"/>
            </basic>
            <basic name="updateDateTime">
                <column name="update_date_time"/>
            </basic>
            <basic name="createDateTime">
                <column name="create_date_time"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
